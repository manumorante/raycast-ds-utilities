{
  "name": "ds-utility",
  "title": "Search for utility",
  "description": "Search for utility",
  "icon": "extension_icon.png",
  "author": "manumorante",
  "repository": "https://github.com/manumorante/raycast-ds-utilities",
  "license": "MIT",
  "commands": [
    {
      "name": "main",
      "title": "Utility",
      "subtitle": "Utilities and tokens",
      "description": "Search utility by name or prpp.",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "projectPath",
      "type": "directory",
      "required": true,
      "title": "Project directory",
      "description": "The Project directory path."
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.75.2",
    "@raycast/utils": "^1.15.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "react-devtools": "^5.2.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
